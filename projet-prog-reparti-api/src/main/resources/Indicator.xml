<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="fr.polytech.projetprogrepartiapi.entities.Indicator">
        <table name="indicator" schema="projetpermis1" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="fkAction">
                <column name="fk_action" precision="11"/>
            </basic>
            <basic name="wording">
                <column name="wording" nullable="false" length="50"/>
            </basic>
            <basic name="valueIfCheck">
                <column name="valueIfCheck" nullable="false" precision="11"/>
            </basic>
            <basic name="valueIfUnCheck">
                <column name="valueIfUnCheck" nullable="false" precision="11"/>
            </basic>
            <many-to-one name="actionByFkAction" optional="true"
                         target-entity="fr.polytech.projetprogrepartiapi.entities.Action">
                <join-column name="fk_action" referenced-column-name="id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
