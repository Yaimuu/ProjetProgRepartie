<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="fr.polytech.projetprogrepartiapi.entities.Utilisateur">
        <table name="utilisateur" schema="projetpermis1" catalog=""/>
        <attributes>
            <id name="numUtil">
                <column name="NumUtil" precision="11"/>
            </id>
            <basic name="nomUtil">
                <column name="NomUtil" length="100"/>
            </basic>
            <basic name="motPasse">
                <column name="MotPasse" length="100"/>
            </basic>
            <basic name="salt">
                <column name="salt" length="100"/>
            </basic>
            <basic name="role">
                <column name="role" length="100"/>
            </basic>
            <basic name="email">
                <column name="email" nullable="false"/>
            </basic>
            <basic name="surname">
                <column name="surname" nullable="false" length="50"/>
            </basic>
            <basic name="forename">
                <column name="forename" nullable="false" length="50"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
