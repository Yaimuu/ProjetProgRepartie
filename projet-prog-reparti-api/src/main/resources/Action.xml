<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="fr.polytech.projetprogrepartiapi.entities.Action">
        <table name="action" schema="projetpermis1" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="fkAction">
                <column name="fk_action" nullable="false" precision="11"/>
            </basic>
            <basic name="wording">
                <column name="wording" nullable="false" length="25"/>
            </basic>
            <basic name="scoreMinimum">
                <column name="scoreMinimum" nullable="false" precision="11"/>
            </basic>
            <many-to-one name="actionByFkAction" optional="true"
                         target-entity="fr.polytech.projetprogrepartiapi.entities.Action">
                <join-column name="fk_action" nullable="false" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="actionsById" mapped-by="actionByFkAction"
                         target-entity="fr.polytech.projetprogrepartiapi.entities.Action"/>
            <one-to-many name="actionMissionsById" mapped-by="actionByFkAction"
                         target-entity="fr.polytech.projetprogrepartiapi.entities.ActionMission"/>
            <one-to-many name="indicatorsById" mapped-by="actionByFkAction"
                         target-entity="fr.polytech.projetprogrepartiapi.entities.Indicator"/>
            <one-to-many name="inscriptionActionsById" mapped-by="actionByFkAction"
                         target-entity="fr.polytech.projetprogrepartiapi.entities.InscriptionAction"/>
        </attributes>
    </entity>
</entity-mappings>
