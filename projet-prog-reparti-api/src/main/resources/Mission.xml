<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="fr.polytech.projetprogrepartiapi.entities.Mission">
        <table name="mission" schema="projetpermis1" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="wording">
                <column name="wording" nullable="false" length="25"/>
            </basic>
            <one-to-many name="actionMissionsById" mapped-by="missionByFkMission"
                         target-entity="fr.polytech.projetprogrepartiapi.entities.ActionMission"/>
            <one-to-many name="inscriptionsById" mapped-by="missionByFkMission"
                         target-entity="fr.polytech.projetprogrepartiapi.entities.Inscription"/>
        </attributes>
    </entity>
</entity-mappings>
